    "nestjs-pino": "^3.1.1",
    "pino": "^8.8.0",
    "pino-http": "^8.3.0",
    "pino-pretty": "^9.1.1",
    "pino-tee": "^0.3.0",
    "prisma": "^4.11.0",
    "redis": "4.3.1",
    "reflect-metadata": "^0.1.13",
