datasource db {
  provider = "sqlite"
  url      = "file:./database/msfiles.db"
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id             Int      @id @default(autoincrement())
  uid            String
  actor          String
  status         String
  action         String
  originalname   String
  objectname     String?
  linked_objects String?
  bucket         String
  parameters     String?
  error_message  String?
  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt()

  @@map("task")
}
